FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
boxToCell
    {
        box (0 0 0) (3 3 1.7);
        fieldValues ( volScalarFieldValue alpha.water 1 );
    }

    boxToCell
    {
        box (0 0 1.5) (0.5 0.5 2);
        fieldValues ( volScalarFieldValue alpha.water 1 );
    }
    
);


